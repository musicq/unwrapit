{
  "name": "unwrapit",
  "version": "2.5.0",
  "description": "A graceful way to handle errors in Typescript",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./rxjs": {
      "types": "./dist/rxjs.d.ts",
      "require": "./dist/rxjs.cjs",
      "import": "./dist/rxjs.js"
    }
  },
  "repository": "musicq/unwrapit",
  "scripts": {
    "build": "tsup src/index.ts src/rxjs.ts --dts --minify --format cjs,esm",
    "prepublish": "npm run build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "typecheck": "vitest --typecheck"
  },
  "keywords": [
    "result type",
    "typescript",
    "error handler"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "author": "musicq <kui.liang.sg@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-v8": "^2.1.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "peerDependencies": {
    "rxjs": "^7.8.1"
  },
  "peerDependenciesMeta": {
    "rxjs": {
      "optional": true
    }
  },
  "dependencies": {
    "panicit": "^2.0.1"
  }
}
